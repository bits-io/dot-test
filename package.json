{
  "name": "no-limit-test",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "nest build && typeorm-ts-node-commonjs migration:generate ./src/database/migrations/migrations -d ./datasource.ts",
    "migration:run": "nest build && typeorm-ts-node-commonjs migration:run -d ./datasource.ts",
    "seed:run": "nest build && ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:run -d ./datasource.ts",
    "seed:create": "ts-node ./node_modules/typeorm-extension/bin/cli.cjs seed:create -r ./src/database/seeds"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.2.1",
    "@nestjs/bullmq": "^10.2.1",
    "@nestjs/microservices": "^10.3.8",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.3.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.3",
    "bullmq": "^5.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "collect.js": "^4.36.1",
    "cors": "^2.8.5",
    "exceljs": "^4.4.0",
    "ioredis": "^5.4.1",
    "moment": "^2.30.1",
    "mssql": "^10.0.4",
    "mysql2": "^3.9.2",
    "nodemailer": "^6.9.14",
    "pdfkit": "^0.15.0",
    "pdfkit-table": "^0.1.99",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.0",
    "typeorm-transactional": "^0.5.0",
    "typeorm-where": "^1.0.1",
    "voilab-pdf-table": "^0.5.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/pdfkit": "^0.13.4",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
